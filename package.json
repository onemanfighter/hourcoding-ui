{
  "name": "@hourcoding/hourcoding-ui",
  "version": "0.0.5",
  "description": "UI library for the hourcoding web apps",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "react-scripts test --silent --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false --coverageThreshold=100",
    "test:watch": "react-scripts test --silent --watchAll=true",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check .",
    "prettier:staged": "prettier --write .",
    "prepare": "husky",
    "lint": "eslint . --ext .ts,.tsx",
    "fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "sb": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "yarn chromatic --project-token $CHROMATIC_TOKEN",
    "publish": "yarn publish --access public",
    "super-minor": "yarn version patch",
    "minor": "yarn version minor",
    "major": "yarn version major",
    "build:dev": "rimraf dist && rollup -c --environment NODE_ENV:development",
    "build:prod": "rimraf dist && rollup -c --environment NODE_ENV:production"
  },
  "homepage": "https://hourcoding-ui.netlify.app/",
  "publishConfig": {
    "access": "public"
  },
  "author": "Amit Raikwar",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "framer-motion": "^11.1.7",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.25",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-controls": "^8.0.5",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/addon-onboarding": "^8.0.5",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^8.0.5",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/blocks": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/react-webpack5": "^8.0.5",
    "@storybook/test": "^8.0.5",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "autoprefixer": "^10.4.19",
    "chromatic": "^11.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.2.0",
    "rollup": "^4.18.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "serve": "^14.2.3",
    "storybook": "^8.0.5",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.3",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "react",
    "hourcoding-ui",
    "hourcoding",
    "hourcoding-react",
    "hour-coding"
  ]
}
